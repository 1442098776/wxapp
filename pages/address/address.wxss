.null{
  width: 100%;
  height: 88rpx;
}
.footer{
  position: fixed;
  right: 0;
  bottom: 0;
  left: 0;
  height: 88rpx;
  line-height: 88rpx;
  background: #FE4543;
  color: #FFFFFF; 
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
}
.address{
  margin: 20rpx 0 20rpx 0;
  background: #fff;
  padding: 31rpx 31rpx 24rpx 29rpx;
  font-family: PingFang-SC-Medium;
}
.address>view:first-child{
  justify-content: space-between;
  font-size: 30rpx;
  color: #353535;
}
.address>view:nth-of-type(2){
  font-size: 24rpx;
  color: #666666;
  margin:34rpx 0 22rpx;
}
.address>view:last-of-type{
  justify-content: space-between;
  color: #999999;
  font-size: 22rpx;
  align-items: center;
}
.address .default>view:first-of-type{
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
}
.address .default>view:first-of-type image{
  width: 100%;
  height: 100%;
}
.address>view:last-of-type>view:last-of-type>view:first-of-type{
  margin-right: 20rpx;
}
.address>view:last-of-type>view:last-of-type>view,.address>view:last-of-type>view:last-of-type>navigator{
  width: 90rpx;
  height: 40rpx;
  flex-direction: column;
  justify-content: center;
  border: 1rpx solid #999999;
  text-align: center;
  border-radius: 20rpx;
}

.popup{
  position:fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 10;
}
.popup>view{
  width: 75%;
  margin:45% auto 0 auto;
  background: #fff;
  padding-top: 50rpx;
  font-family: PingFang-SC-Medium;
  border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  -moz-border-radius: 8rpx;
  -ms-border-radius: 8rpx;
  -o-border-radius: 8rpx;
}
.popup>view>view:not(:last-of-type){
  justify-content: space-between;
  padding:0 70rpx 0 60rpx;
}
.popup>view>view:first-of-type{
  margin-bottom: 67rpx;
}
.popup>view>view:nth-of-type(2){
  margin-bottom: 54rpx;
}
.popup>view>view text{
  flex-direction: column;
  justify-content: center;
  font-size: 30rpx;
  color: #353535; 
}
.popup>view>view>view{
  width: 16rpx;
  height: 28rpx;
}
.popup>view>view>view>image{
  width: 100%;
  height: 100%;
}
.popup>view>view:last-of-type{
  height: 100rpx;
  line-height: 100rpx;
  color: #00C200;
  font-size: 36rpx;
  border-top: 1rpx solid #EAEAEA;
}