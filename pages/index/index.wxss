.search {
  width: 690rpx;
  height: 68rpx;
  line-height: 68rpx;
  margin: 15rpx auto 15rpx auto;
  border-radius: 34px;
  -webkit-border-radius: 34px;
  -moz-border-radius: 34px;
  -ms-border-radius: 34px;
  -o-border-radius: 34px;
  background: #fff;
  color: #bbb;
  padding-left: 71rpx;
  font-size: 30rpx;
}

.search>image {
  position: absolute;
  top: 15rpx;
  left: 18rpx;
  margin-bottom: 20rpx;
  width: 35rpx;
  height: 37rpx;
}

