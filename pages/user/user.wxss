.popup,.burseCount{
  position: fixed;
  top: 0;
  right: 0;
  bottom:0rpx;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 40;
}
.popup>view{
  width: 89%;
  height: 800rpx;
  margin:20% auto 0 auto;
}
.popup .top{
  width: 100%;
  height: 200rpx;
}
.popup .top>image{
  width: 100%;
  height: 100%;
}
.popup .close{
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  line-height: 40rpx;
  text-align: center;
  top: -25rpx;
  right: -25rpx;
  background: #fff;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
}
.popup .close image{
  width: 20rpx;
  height: 20rpx;
}
.popup .contenCount{
  width: 100%;
  height:600rpx;
  background: #fff;
}
.popup .middle{
  padding:73rpx 0 95rpx 0;
  color: #353535;
  font-size: 30rpx;
}
.popup .middle image{
  display: block;
  width: 96rpx;
  height: 77rpx;
  margin:0 auto 22rpx auto;
}
.popup .bottom{
  padding:0 30rpx 74rpx;
  font-size: 24rpx;
}
.popup .bottom>view{
  width: 10%;
  height: 80rpx;
  color: #FE5443;
  border:1rpx solid #FE5443;
  margin-right: 5%;
  border-radius: 10rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  -ms-border-radius: 10rpx;
  -o-border-radius: 10rpx;
}
.popup .bottom>view:last-of-type{
  margin-right: 0;
}
.popup .bottom>view>view:first-of-type{
  margin-bottom: 10rpx;
}
.popup .bottom .nullBorder{
  margin:0 auto 1rpx auto;
  width: 29rpx;
  height: 8rpx;
  border:1rpx solid #FE5443;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
}
.popup .bottom .img{
  width: 28rpx;
  height: 27rpx;
  display: block;
  margin:0 auto;
}
.popup .bottom .active{
  background: #FE5443;
  border-color: #E53220;
  color: #fff;
}
.popup .bottom .past{
  background: #fff;
  border-color: #999999;
  color: #999999;
}
.popup .bottom .past .nullBorder{
  border-color: #999999;
}
.popup navigator{
  width: 48%;
  height: 70rpx;
  line-height: 70rpx;
  color: #FE5443;
  font-size: 32rpx;
  margin:0 auto 20rpx auto;
  border:1rpx solid #FE5443;
  border-radius: 35rpx;
  -webkit-border-radius: 35rpx;
  -moz-border-radius: 35rpx;
  -ms-border-radius: 35rpx;
  -o-border-radius: 35rpx;
}
.popup .integral{
  font-size: 24rpx;
  color: #353535;
}

.burseCount>view{
  width: 92%;
  margin:16% auto 0 auto;
  background: #fff;
}
.burseCount .burseSection{
  padding:40rpx 20rpx 30rpx;
  border-bottom: 2rpx solid #EAEAEA;
  color: #353535;
  font-size: 30rpx;
}
.burseCount .burseSection image{
  width: 29rpx;
  height: 28rpx;
}
.burseCount .burseSection1{
  padding:0 30rpx 24rpx;
}
.burseCount .burseSection1 .title{
  margin-top: 30rpx;
  color: #666666;
  font-size: 26rpx;
}
.burseCount .burseSection1 .content{
  color: #999999;
  font-size: 22rpx;
  margin: 20rpx 0 20rpx;
}



.header .polygon{
  position: absolute;
  top: 0;
  right: 70rpx;
  width: 56rpx;
  height: 120rpx;
}
.header .polygon image{
  width: 56rpx;
  height: 119rpx;
}
.header{
  width: 100%;
  height: 400rpx;
  padding-top: 55rpx;
  color: #353535;
  font-family: PingFang-SC-Medium;
  background:url("data:image/png;base64,") no-repeat;
  background-size:100% 100%;
}
.header>view{
  width: 23%;
  margin:0 auto;
}
.header .img{
  width: 160rpx;
  height: 160rpx;
  margin:0 auto 22rpx auto;
}
.header .img>image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
}
.header button{
  font-size: 30rpx !important;
  background: none;
  padding:0;
  border:0;
}
.header .getUserInfo{
  line-height: 40rpx;
}

button::after{
  border:none;
  outline: none;
}

.header .fans{
  margin-top:30rpx; 
}
.header .fans image{
  width: 27rpx;
  height: 27rpx;
}
.header .fans>view{
  flex: 1;
}
.header .fans text{
  padding-left: 10rpx;
  font-size: 22rpx;
  color: #666666;
}

.section{
  width: 100%;
  background: #fff;
}
.allOrder{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  padding:0 30rpx;
  justify-content: space-between;
}
.allOrder view,.allOrder view image{
  width: 16rpx;
  height: 28rpx;
}

.order{
  width: 100%;
  height: 170rpx;
  color: #353535;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
}
.order>navigator{
  width: 20%;
}
.order .img{
  width: 50rpx;
  height: 51rpx;
  margin:40rpx auto 25rpx auto;
  position: relative;
}
.order .img image{
  width: 100%;
  height: 100%;
}
.order .img .num{
  position: absolute;
  width: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  background: #F12C20;
  z-index: 10;
  top: -10rpx;
  right: -10rpx;
  color: #FFFFFF;
  font-size: 17rpx;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 1;
overflow: hidden;
}
.section1{
  width: 100%;
  background: #fff;
  padding:30rpx 30rpx 72rpx;
  color: #353535;
  margin:20rpx 0 20rpx;
}
.section1>view:first-of-type{
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
}
.section1>view:first-of-type image{
  width: 42rpx;
  height: 42rpx;
}
.section1>view:last-of-type{
  margin-top: 74rpx;
}
.section1>view:last-of-type>view:first-of-type{
  font-size: 80rpx;
  margin-bottom: 33rpx;
}
.section1>view:last-of-type>view:last-of-type{
  font-size: 22rpx;
}

.section2{
  color: #353535;
  font-size:  22rpx;
  background: #fff;
  margin-bottom: 51rpx;
}
.section2>view:first-of-type{
  border-bottom: 1rpx solid #eaeaea;
  padding-bottom: 30rpx;
}
.section2 .count{
  width: 25%;
}
.section2 .count view:first-of-type{
  width: 52rpx;
  height: 52rpx;
  margin:43rpx auto 34rpx auto;
}
.section2 .count image{
  width: 100%;
  height: 100%;
}
.section2 .count view:last-of-type{
  line-height: 22rpx;
}
.section2 .count>button{
  border:0;
  background: none;
  padding:0;
  line-height: auto !important;
  font-size: 22rpx !important;
  color: #353535 !important;
}
.section2>view:last-of-type{
  padding-bottom: 30rpx;
}